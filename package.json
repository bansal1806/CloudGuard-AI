{
  "name": "cloudguard-ai",
  "version": "1.0.0",
  "description": "Next-generation AI-powered cloud management platform",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:integration": "jest --config jest.integration.config.js",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:seed": "prisma db seed",
    "db:studio": "prisma studio",
    "docker:build": "docker build -t cloudguard-ai .",
    "docker:run": "docker run -p 3000:3000 cloudguard-ai",
    "docker:compose": "docker-compose up -d",
    "docker:compose:down": "docker-compose down",
    "docker:compose:full": "docker-compose -f docker-compose.full.yml up -d",
    "k8s:deploy": "kubectl apply -f kubernetes/",
    "k8s:delete": "kubectl delete -f kubernetes/",
    "analyze": "ANALYZE=true npm run build",
    "clean": "rm -rf .next node_modules/.cache",
    "prepare": "husky install"
  },
  "dependencies": {
    "@aws-sdk/client-cloudwatch": "^3.490.0",
    "@aws-sdk/client-ec2": "^3.490.0",
    "@aws-sdk/client-elastic-load-balancing-v2": "^3.490.0",
    "@aws-sdk/client-rds": "^3.490.0",
    "@aws-sdk/client-s3": "^3.490.0",
    "@google-cloud/compute": "^4.1.0",
    "@google-cloud/monitoring": "^4.0.0",
    "@hookform/resolvers": "^3.3.2",
    "@prisma/client": "^5.7.1",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-progress": "^1.0.3",
